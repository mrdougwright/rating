<div class="hero-unit">
  <h1>Ratings</h1><br />

  <table class="table">
    <th>name</th>
    <th>id</th>
    <th>date created</th>
    <th>Rate!</th>
  <% @names.each do |name| %>
    <tr>
      <td><%= name.name %></td>
      <td><%= name.id %></td>
      <td><%= name.created_at.to_s :short %></td>
    </tr>
  <% end %>
  </table>

  <br />
</div>